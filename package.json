{
  "name": "icon",
  "version": "0.4.9",
  "description": "ma icons management",
  "main": "dist/index.js",
  "types": "src/index.d.js",
  "scripts": {
    "test": "jest --coverage",
    "fetch": "node scripts/fetch.js",
    "generate": "rm -rf src/icons && rm -rf src/rn && node scripts/generate.js",
    "docs": "webpack -c build/webpack.docs.config.js",
    "dev": "webpack serve -c build/webpack.dev.config.js",
    "build:cjs": "babel ./src/icons --out-dir ./src/cjs --config-file ./build/babel.cjs.config.js --keep-file-extension --copy-files --no-babelrc",
    "build": "webpack -c build/webpack.prod.config.js",
    "lint": "eslint scripts build demo test --fix"
  },
  "repository": {
    "type": "git",
    "url": "git@git.woa.com:timkit/ma-icons.git"
  },
  "keywords": [
    "icon"
  ],
  "author": "weiitang",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.5",
    "@babel/plugin-transform-arrow-functions": "^7.16.5",
    "@babel/plugin-transform-modules-commonjs": "^7.16.5",
    "@babel/preset-env": "^7.16.8",
    "@babel/preset-react": "^7.16.7",
    "@svgr/core": "^6.1.2",
    "@svgr/plugin-svgo": "^6.2.0",
    "axios": "^0.24.0",
    "babel-jest": "^27.4.6",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.5.1",
    "dotenv": "^10.0.0",
    "eslint": "^8.25.0",
    "figma-js": "^1.14.0",
    "fs-extra": "^10.0.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.4.7",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "style-loader": "^3.3.1",
    "styled-components": "^5.3.3",
    "uppercamelcase": "^3.0.0",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.0"
  },
  "peerDependencies": {
    "react": "^17.0.2"
  }
}
